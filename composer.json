        "algolia/algoliasearch-client-php": "^3.1",
        "storyblok/php-client": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Digitlimit\\StoryblokAlgolia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digitlimit\\StoryblokAlgolia\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digitlimit\\StoryblokAlgolia\\StoryblokAlgoliaServiceProvider"
            ],
            "aliases": {
                "StoryblokAlgolia": "Digitlimit\\StoryblokAlgolia\\Facades\\StoryblokAlgolia"
            }
        }
    }
